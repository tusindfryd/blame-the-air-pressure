{
  "expo": {
    "name": "Blame the Air Pressure",
    "slug": "blame-the-air-pressure",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "io.blametheairpressure",
      "versionCode": 2,
      "permissions": [
        "LOCATION",
        "MOTION"
      ]
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
